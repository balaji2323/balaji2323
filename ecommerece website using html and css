    <script>
        var menuitems=document.getElementById("menu  items");
        menuitems.style.maxHeight ="0px";
        function menutoggle(){
            if(men items.style.maxHeight=="0px")
        {
            menuitems.style.maxHeight="200px";
        }
        else
        {
            menuitems.style.maxHeight=="0px";
        }
        }
    </script>

